# vim: ft=bash

# Shortcuts to frequently used directories
alias pl="cd ~/Documents/playground/"
alias lc="cd /usr/LocalDisk/"
alias st="cd ~/Documents/suckless-utils/st/"
alias dwm="cd ~/Documents/suckless-utils/dwm"
alias cfg="cd $CONFIG_REPO_PATH"
alias nt="cd ~/Documents/Notes/"
alias dl="cd ~/Downloads"
alias gpd="gitpagediff"

alias t=tree

alias vbox="VBoxManage"
